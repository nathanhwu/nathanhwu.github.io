---
const description = "404";
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';

import ThemeIcon from '../components/ThemeIcon.astro';

import Head from '../components/Head.astro';
import Script from '../components/Script.astro';
import Layout from "../layouts/Layout.astro";

const pageTitle = "Nathan Wu | 404: Page Not Found";

---
<!DOCTYPE html>

<Head pageTitle = {pageTitle}/>

<html lang="en">
    <body>
        <main class="lost">
            <div class="lost_inner content">
                <img src="/images/ohno.gif" role="presentation" width="350" height="259" decoding="async";                >
                <h1 class="lost__header">{ description }</h1>
                <p class="lost__body">Scream.<br> You shouldn't be here.</p>                
                <a class="lost__link" href="/">Return Home</a>
            </div>
        </main>
    </body>
</html>
